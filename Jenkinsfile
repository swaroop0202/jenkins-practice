pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo this is build stage'
            }
        }
        stage('Test') {
            steps {
                sh 'echo this is test stage'
            }
        }
        stage('Deploy') {
            steps {
                sh 'this is deploy stage'
            }
        }
    }
}
